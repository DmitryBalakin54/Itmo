        <mi>x</mi>
        <mo stretchy="false">)</mo>
        <mo stretchy="false">&#x21D2;<!-- ⇒ --></mo>
        <mi>P</mi>
        <mo stretchy="false">(</mo>
        <msup>
          <mi>x</mi>
          <mo>&#x2032;</mo>
        </msup>
        <mo stretchy="false">)</mo>
        <mo stretchy="false">)</mo>
