      <mstyle displaystyle="true" scriptlevel="0">
        <mi>B</mi>
        <mo>=</mo>
        <mi>C</mi>
      </mstyle>
    </mrow>
    <annotation encoding="application/x-tex">{\displaystyle B=C}</annotation>
  </semantics>
